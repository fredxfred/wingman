{
  "name": "cloud-wingman",
  "displayName": "CloudWingman",
  "version": "0.1.0",
  "description": "Your cloud wingman. Supports OpenAI, Anthropic, or any LLM on your local inference server.",
  "categories": [
    "Other"
  ],
  "keywords": [
    "ai",
    "gpt",
    "gpt3",
    "gpt4",
    "llm",
    "chatgpt",
    "chatgpt4",
    "chatgpt3",
    "claude",
    "openai",
    "anthropic",
    "refactoring",
    "debugging",
    "generation",
    "codegen",
    "copilot",
    "wingman",
    "mplode",
    "mplodeai",
    "cloud",
    "cloudwingman",
    "faas",
    "lambda",
    "gcp",
    "aws",
    "azure",
    "functions",
    "swe"
  ],
  "homepage": "https://github.com/fredxfred/wingman/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/fredxfred/wingman"
  },
  "license": "ISC",
  "author": {
    "name": "Fred"
  },
  "publisher": "mplodeai",
  "type": "commonjs",
  "main": "dist/extension.umd.js",
  "browser": "dist/extension.browser.js",
  "scripts": {
    "build": "tsx scripts/esbuild.ts",
    "build:watch": "tsx scripts/esbuild.ts --watch",
    "build:browser": "tsx scripts/esbuild.ts --browser",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp -c \"Release v%s\" package.json"
  },
  "contributes": {
    "commands": [
      {
        "command": "wingman.setApiKey",
        "title": "Wingman: Set API Key"
      }
    ],
    "configuration": {
      "title": "Wingman"
    },
    "views": {
      "wingman": [
        {
          "type": "webview",
          "id": "wingman.mainView",
          "name": "Wingman"
        }
      ]
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "wingman",
          "title": "Wingman",
          "icon": "assets/GameIconsFeatheredWing.svg"
        }
      ]
    }
  },
  "activationEvents": [],
  "dependencies": {
    "@ai-zen/node-fetch-event-source": "^2.1.2",
    "@anthropic-ai/sdk": "^0.20.1",
    "gpt-tokenizer": "^2.1.2",
    "llama-tokenizer-js": "^1.1.3",
    "p-timeout": "^6.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.84.2",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "esbuild": "^0.19.8",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "vscode": "^1.84.2"
  },
  "icon": "assets/logo.png",
  "badges": [
    {
      "description": "PRs Welcome",
      "url": "https://img.shields.io/badge/PRs-welcome-brightgreen.svg",
      "href": "https://github.com/fredxfred/wingman/blob/main/README.md"
    }
  ]
}
